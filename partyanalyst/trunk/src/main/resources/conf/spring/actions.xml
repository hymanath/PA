<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC 
	"-//SPRING//DTD BEAN//EN" 
	"http://www.springframework.org/dtd/spring-beans.dtd">
	
<beans>
	<!-- Example of SAF2 action instantiated by Spring -->
	<bean id="helloWorldAction" class="com.itgrids.partyanalyst.HelloWorldAction"
		singleton="false" >
		<property name="blogService" ref="blogService" />
	</bean>
	<bean id="partyPerformanceReportAction" class="com.itgrids.partyanalyst.PartyPerformanceReportAction"
		singleton="false" >
		<property name="partyService" ref="partyService" />
	</bean>
	<bean id="partyPerformanceJasperAction" class="com.itgrids.partyanalyst.PartyPerformanceJasperAction"
		singleton="false" >
	</bean>
</beans>




